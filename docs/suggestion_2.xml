<exporter id="">
	<property id=""><!--min 1--><!--*-->
		<description>A short description of the property.</description><!-- Plain Text description file. This will be truncated after 255 characters -->
		<image external="true/false">http://homestreet24.ch/</image><!-- Full image URL to main Image file -->
		<name>The name of the item.</name><!-- Sales or descriptive searchable name -->
		<url>URL of the item.</url><!-- Requested Target URL of property (Portals are not required to use this value if they do not wish to promote a target service) -->
		<productID>123487</productID><!--*--><!-- The ID that the exporter decides upon for cross referenzing. This Value must be unique to the exporters property -->
		<transferOwnerID></transferOwnerID><!--*--><!-- This determines the account that is responsible for the property (this account should be registarable at a central location?)  -->
		<releaseDate></releaseDate><!-- The Date the Property has/will become available for sale a value in the past will make the item available immiedialy, if there is no value or the element is missing, it will be considered as a "per request" item  -->
		<offer main="true/false"><!--*--><!-- Multiple offers are possible first encountered main will be used as main offer (or only offer depending on the recipient) if there is no main specified the first one will be considered as main. -->
			<type>ENUM(buy,rent)</type><!--*--><!-- warming! a missing value will consider "buy" as a value, or will not be syncroniced at all -->
			<availability>Discontinued/InStock</availability>
			<availabilityStarts>DateTime</availabilityStarts>
			<category>house/Office block</category>
			<price frequency="NUMERIC" unit="ENUM(m,w,d,y,h,infinite)"><!-- DECIMAL(2) || free --></price> <!-- default frequency = 0, default unit = 'infinite', empty/missing/0 element will be considered "per request", string 'free' can be submited -->
			<priceCurrency>in 3-letter ISO 4217 format</priceCurrency><!-- no currency will result in 0 price value -->
			<seller><!--*-->
				<organization>
					<address>
						<addressCountry>ISO 3166-1 alpha-2 country code.</addressCountry>
						<addressLocality>The locality. For example, Mountain View.</addressLocality>
						<addressRegion>The region. For example, CA.</addressRegion>
						<postalCode>The postal code. For example, 94043.</postalCode>
						<postOfficeBoxNumber>The post offce box number for PO box addresses.</postOfficeBoxNumber>
						<streetAddress>The street address. For example, 1600 Amphitheatre Pkwy.</streetAddress>
					</address>
					<legalName>Betterhomes</legalName>
					<logo>URL</logo>
				</organization>
				<person>
					<givenName></givenName>
					<familyName></familyName>
					<email></email>
					<faxNumber></faxNumber>
					<telephone></telephone>
					<gender></gender>
				</person>
			</seller>
		</offer>
		<place>
			<address>
				<addressCountry>ISO 3166-1 alpha-2 country code.</addressCountry>
				<addressLocality>The locality. For example, Mountain View.</addressLocality>
				<addressRegion>The region. For example, CA.</addressRegion>
				<postalCode>The postal code. For example, 94043.</postalCode>
				<postOfficeBoxNumber>The post offce box number for PO box addresses.</postOfficeBoxNumber>
				<streetAddress>The street address. For example, 1600 Amphitheatre Pkwy.</streetAddress>
			</address>
			<contactpoint>
				<contactType>ENUM('visit,landlord')</contactType>
				<givenName></givenName>
				<familyName></familyName>
				<email>Email address.</email>
				<faxNumber>The fax number.</faxNumber>
				<telephone>The telephone number.</telephone>
			</contactpoint>
			<geo>
				<latitude></latitude>
				<longitude></longitude>
			</geo>
			<attachments> <!-- all refering documents belong here -->
				<media type="ENUM(image)" external="external" order="1">
					<description></description>
					<image></image>
					<name></name>
					<url></url> <!-- if internal '/attachment/' is root -->
					<caption></caption> 
				</media>
				<media type="ENUM(image)" order="2">
					<description></description>
					<image></image>
					<name></name>
					<url></url>
					<caption></caption>
				</media>
				<media type="ENUM(image)" order="3">
					<description></description>
					<image></image>
					<name></name>
					<url></url>
					<caption></caption>
				</media>
			</attachments>
			<features>
				<feature key="(required)" value="(optional)" />
			</features>
		</place>
	</property>
</exporter>